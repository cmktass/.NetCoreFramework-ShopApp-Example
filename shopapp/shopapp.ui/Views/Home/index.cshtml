@model List<shopapp.entity.Product>

@{
  Layout="_layout";
}

  @await Html.PartialAsync("_product",@Model)
  <div class="row">
  <div class="col">
    <nav aria-label="Page navigation example">
  <ul class="pagination">
        <li class="page-item"><a class="page-link" href="#">Previous</a></li>
        @for (int i = 0; i < @ViewBag.p.totalPages(); i++)
        {
             <li class=""><a class="page-link" href="?page=@(i+1)">@(i+1)</a></li>
        }
        <li class="page-item"><a class="page-link" href="#">Next</a></li>
  </ul>
</nav>
  </div>
</div>